programa{
  funcao inicio(){
    inteiro entrada = 0, contador = 1, saida = 0 
    escreva ("Digite o número que você gostaria de conhecer a tabuada:\n")
    leia(entrada)
    enquanto(contador<=10){
        saida = entrada * contador
        escreva(saida ,"---")
        contador++
    }
  }
    


}
  